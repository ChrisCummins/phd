    t[12] += q[3] * s[3][12];
    t[13] += q[3] * s[3][13];
    t[14] += q[3] * s[3][14];
    t[15] += q[3] * s[3][15];
  } while (0);

  for (int o = 0; o < 16; o++, e += f) {
    e[0] = h * t[o] + i * e[0];
  }
}