      t[1] += u[3] * s[3][1];
      t[2] += u[3] * s[3][2];
      t[3] += u[3] * s[3][3];
      t[4] += u[3] * s[3][4];
      t[5] += u[3] * s[3][5];
      t[6] += u[3] * s[3][6];
      t[7] += u[3] * s[3][7];
      t[8] += u[3] * s[3][8];
      t[9] += u[3] * s[3][9];
      t[10] += u[3] * s[3][10];
      t[11] += u[3] * s[3][11];
      t[12] += u[3] * s[3][12];
      t[13] += u[3] * s[3][13];
      t[14] += u[3] * s[3][14];
      t[15] += u[3] * s[3][15];
    } while (0);

    a += 4 * b;
    c += 4 * d;
    p += 4 * d;
    barrier(1);

  } while (p < g * d);

  s[k][j] = r;
  barrier(1);

  do {
    t[0] += q[0] * s[0][0];
    t[1] += q[0] * s[0][1];
    t[2] += q[0] * s[0][2];
    t[3] += q[0] * s[0][3];
    t[4] += q[0] * s[0][4];
    t[5] += q[0] * s[0][5];
