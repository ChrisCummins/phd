      t[9] += u[1] * s[1][9];
      t[10] += u[1] * s[1][10];
      t[11] += u[1] * s[1][11];
      t[12] += u[1] * s[1][12];
      t[13] += u[1] * s[1][13];
      t[14] += u[1] * s[1][14];
